import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
    public static int countOccurence(String s, char ch) {
        int count=0;
        for(int i=0;i<s.length();i++) {
            if(s.charAt(i) == ch) {
                count++;
            }
        }
        return count;
    }
  public static void main (String[] args) throws java.lang.Exception
  {
    String s ="helloworld";
    char ch = 'l';
    
    System.out.println(countOccurence(s, ch));

  }
}
